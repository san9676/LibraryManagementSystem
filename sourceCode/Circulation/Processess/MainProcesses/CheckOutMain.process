<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:UUID="java://Custom.Custom" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.example.org/Circulation">
    <xsd:import namespace="http://www.example.org/Circulation" schemaLocation="/SharedResources/Schemas/Circulation.xsd"/>
    <pd:name>Processess/MainProcesses/CheckOutMain.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:BookCheckOutReq"/>
    <pd:startX>122</pd:startX>
    <pd:startY>139</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns1:ErrorReport)">
                <group>
                    <pfx:fault>
                        <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                            <pfx:FaultCode>
                                <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                            </pfx:FaultCode>
                        </xsl:if>
                        <xsl:if test="$_error/ns1:ErrorReport/Msg">
                            <pfx:FaultMsg>
                                <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                            </pfx:FaultMsg>
                        </xsl:if>
                    </pfx:fault>
                </group>
            </xsl:when>
            <xsl:when test="exists($RenewBook/pfx:BookCheckOutRes/pfx:transaction_id)">
                <group>
                    <xsl:copy-of select="$RenewBook/pfx:BookCheckOutRes"/>
                </group>
            </xsl:when>
            <xsl:otherwise>
                <group>
                    <xsl:copy-of select="$CheckOutSub/pfx:BookCheckOutRes"/>
                </group>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="group">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx:BookCheckOutRes" minOccurs="0"/>
                    <xsd:element ref="pfx:fault" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>869</pd:endX>
    <pd:endY>143</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1679048065681</pd:targetNamespace>
    <pd:activity name="CheckOutReqLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>346</pd:x>
        <pd:y>143</pd:y>
        <config>
            <processName>/LoggerProcesses/LogHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'CirculationModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'CheckOutBook'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'INFO'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'Started'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="$SetVariable/root/param"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'RequestPayload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd',current-date())"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($Start/pfx:BookCheckOutReq, true())"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>214</pd:x>
        <pd:y>141</pd:y>
        <config>
            <variableConfig>/SharedResources/SharedVariables/CorrealationID.jobsharedvariable</variableConfig>
            <showResult>true</showResult>
        </config>
        <pd:inputBindings>
            <root>
                <param>
                    <xsl:value-of select="UUID:getUUID()"/>
                </param>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="CheckOutSub">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>590</pd:x>
        <pd:y>82</pd:y>
        <config>
            <processName>/Processess/SubProcesses/CheckOutSub.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx:BookCheckOutReq"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="CheckOutResLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>719</pd:x>
        <pd:y>143</pd:y>
        <config>
            <processName>/LoggerProcesses/LogHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'CirculationModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'CheckOutBook'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'INFO'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'Success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="$SetVariable/root/param"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'ResponsePayload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd',current-date())"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml(if (exists($RenewBook)) then $RenewBook/pfx:BookCheckOutRes else $CheckOutSub/pfx:BookCheckOutRes, true())"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>349</pd:x>
        <pd:y>243</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="string-length($Start/pfx:BookCheckOutReq/pfx:library_card_number)=0">
                    <ns2:ActivityInput>
                        <message>
                            <xsl:value-of select="'Library Card Number can not be null'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat('101', '-','400')"/>
                        </messageCode>
                    </ns2:ActivityInput>
                </xsl:when>
                <xsl:otherwise>
                    <ns2:ActivityInput>
                        <message>
                            <xsl:value-of select="'BookID can not be null'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat('102', '-','400')"/>
                        </messageCode>
                    </ns2:ActivityInput>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>560</pd:x>
        <pd:y>286</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="CheckOutErrLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>737</pd:x>
        <pd:y>288</pd:y>
        <config>
            <processName>/LoggerProcesses/ErrLogHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns:ErrorLogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'CirculationModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'CheckOutBook'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'ERROR'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'Failed'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="$SetVariable/root/param"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'ErrorDetails'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd',current-date())"/>
                </ns:Timestamp>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <ns:ErrorCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </ns:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <ns:ErrorMsg>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns:ErrorMsg>
                </xsl:if>
            </ns:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>459</pd:x>
        <pd:y>146</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="RenewBook">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>594</pd:x>
        <pd:y>177</pd:y>
        <config>
            <processName>/Processess/SubProcesses/RenewBook.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx:BookCheckOutReq"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>CheckOutReqLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CheckOutReqLog</pd:from>
        <pd:to>Null</pd:to>
        <pd:xpathDescription>validate</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>string-length($Start/pfx:BookCheckOutReq/pfx:library_card_number)!=0 
and
string-length($Start/pfx:BookCheckOutReq/pfx:book_id)!=0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>CheckOutSub</pd:from>
        <pd:to>CheckOutResLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CheckOutResLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CheckOutReqLog</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>CheckOutErrLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CheckOutErrLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>CheckOutSub</pd:to>
        <pd:xpathDescription>Not renew</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx:BookCheckOutReq/pfx:Renew/pfx:isRenew='false'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>RenewBook</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RenewBook</pd:from>
        <pd:to>CheckOutResLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>