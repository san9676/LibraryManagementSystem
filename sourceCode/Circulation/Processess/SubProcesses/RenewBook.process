<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.example.org/Circulation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.example.org/Circulation" schemaLocation="/SharedResources/Schemas/Circulation.xsd"/>
    <pd:name>Processess/SubProcesses/RenewBook.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:BookCheckOutReq"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$Map-Data/pfx:BookCheckOutRes"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:BookCheckOutRes"/>
    <pd:endX>672</pd:endX>
    <pd:endY>63</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1679053973456</pd:targetNamespace>
    <pd:activity name="fetchBorrowingDetails">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>259</pd:x>
        <pd:y>57</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT TRANID,BOOKID,STATUS,CARDNUMBER
  FROM LIB_BORROWING where TRANID=?
</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>TRANID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <QueryOutputCachedSchemaColumns>TRANID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>BOOKID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>STATUS</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>CARDNUMBER</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <TRANID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:BookCheckOutReq/pfx:Renew/pfx:transactionID)">
                            <xsl:value-of select="$Start/pfx:BookCheckOutReq/pfx:Renew/pfx:transactionID"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </TRANID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="UpdateBorrowingDetails">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>528</pd:x>
        <pd:y>59</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>UPDATE LIB_BORROWING
  SET DUE_DATE = ? and STATUS=? where TRANID=?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>DUE_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>STATUS</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>TRANID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <DUE_DATE>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:due_date)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:due_date"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </DUE_DATE>
                <STATUS>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:status)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:status"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </STATUS>
                <TRANID>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:transaction_id)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:transaction_id"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </TRANID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>388</pd:x>
        <pd:y>65</pd:y>
        <config>
            <element ref="pfx:BookCheckOutRes"/>
        </config>
        <pd:inputBindings>
            <pfx:BookCheckOutRes>
                <xsl:if test="$fetchBorrowingDetails/resultSet/Record[1]/TRANID">
                    <pfx:transaction_id>
                        <xsl:value-of select="$fetchBorrowingDetails/resultSet/Record[1]/TRANID"/>
                    </pfx:transaction_id>
                </xsl:if>
                <xsl:if test="$fetchBorrowingDetails/resultSet/Record[1]/BOOKID">
                    <pfx:book_id>
                        <xsl:value-of select="$fetchBorrowingDetails/resultSet/Record[1]/BOOKID"/>
                    </pfx:book_id>
                </xsl:if>
                <pfx:due_date>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd',tib:add-to-date(current-date(),0,0,14))"/>
                </pfx:due_date>
                <pfx:status>
                    <xsl:value-of select="'Renewed'"/>
                </pfx:status>
            </pfx:BookCheckOutRes>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>fetchBorrowingDetails</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>fetchBorrowingDetails</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>UpdateBorrowingDetails</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>UpdateBorrowingDetails</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>