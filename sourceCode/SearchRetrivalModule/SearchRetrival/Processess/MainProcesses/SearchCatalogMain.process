<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://www.eniquesolutions.com/V1/SearchRetrival.xsd" xmlns:cust="java://Custom.Custom" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.eniquesolutions.com/V1/SearchRetrival.xsd" schemaLocation="/SharedResources/Schemas/SearchRetrival.xsd"/>
    <pd:name>Processess/MainProcesses/SearchCatalogMain.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:SearchReq"/>
    <pd:startX>100</pd:startX>
    <pd:startY>74</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns1:ErrorReport)">
                <pfx:SearchCatalogResults>
                    <pfx:BookDetails/>
                    <pfx:control>
                        <xsl:if test="$Start/pfx:SearchReq/pfx:sort_by">
                            <pfx:sort_by>
                                <xsl:value-of select="$Start/pfx:SearchReq/pfx:sort_by"/>
                            </pfx:sort_by>
                        </xsl:if>
                        <xsl:if test="$Start/pfx:SearchReq/pfx:sort_order">
                            <pfx:sort_order>
                                <xsl:value-of select="$Start/pfx:SearchReq/pfx:sort_order"/>
                            </pfx:sort_order>
                        </xsl:if>
                        <xsl:if test="$Start/pfx:SearchReq/pfx:page_number">
                            <pfx:page_number>
                                <xsl:value-of select="$Start/pfx:SearchReq/pfx:page_number"/>
                            </pfx:page_number>
                        </xsl:if>
                        <xsl:if test="$Start/pfx:SearchReq/pfx:page_size">
                            <pfx:page_size>
                                <xsl:value-of select="$Start/pfx:SearchReq/pfx:page_size"/>
                            </pfx:page_size>
                        </xsl:if>
                        <pfx:nextpageURL/>
                        <pfx:ErrorCode>
                            <xsl:choose>
                                <xsl:when test="exists($_error/ns1:ErrorReport/MsgCode)">
                                    <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </pfx:ErrorCode>
                        <pfx:ErrorMsg>
                            <xsl:choose>
                                <xsl:when test="exists($_error/ns1:ErrorReport/Msg)">
                                    <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </pfx:ErrorMsg>
                    </pfx:control>
                </pfx:SearchCatalogResults>
            </xsl:when>
            <xsl:otherwise>
                <xsl:copy-of select="$SearchCatalogSub/pfx:SearchCatalogResults"/>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:SearchCatalogResults"/>
    <pd:endX>840</pd:endX>
    <pd:endY>76</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1679889798870</pd:targetNamespace>
    <pd:activity name="SearchCatalogReqLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>353</pd:x>
        <pd:y>74</pd:y>
        <config>
            <processName>/LoggerProcesses/LogHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'SearchRetrival'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'SearchCatalog'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'INFO'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'Started'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="$SetCorrID/root/param"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'RequestPayload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd', current-date())"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($Start/pfx:SearchReq, true())"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SetCorrID">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>211</pd:x>
        <pd:y>73</pd:y>
        <config>
            <showResult>true</showResult>
            <variableConfig>/SharedResources/SharedVariables/CorrealationID.jobsharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <root>
                <param>
                    <xsl:value-of select="cust:getUUID()"/>
                </param>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SearchCatalogSub">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>512</pd:x>
        <pd:y>76</pd:y>
        <config>
            <processName>/Processess/SubProcesses/SearchCatalogSub.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx:SearchReq"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SearchCatalogResLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>668</pd:x>
        <pd:y>78</pd:y>
        <config>
            <processName>/LoggerProcesses/LogHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'SearchRetrival'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'SearchCatalog'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'INFO'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'Success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="$SetCorrID/root/param"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'ResponsePayload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd', current-date())"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($SearchCatalogSub/pfx:SearchCatalogResults, true())"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>358</pd:x>
        <pd:y>179</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="string-length($Start/pfx:SearchReq/pfx:Search_Query)=0">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="'Search Query cannot be null'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/SQueryErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:when test="string-length($Start/pfx:SearchReq/pfx:Search_Type)=0">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="'Search Type cannot be null'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/STypeErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:when test="not((upper-case($Start/pfx:SearchReq/pfx:sort_by)='TITLE' or upper-case($Start/pfx:SearchReq/pfx:sort_by)='AUTHOR'))">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="&quot;Sort by can only accept 'TITLE' and 'AUTHOR' as value&quot;"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/sortByErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:when test="not((upper-case($Start/pfx:SearchReq/pfx:sort_order)='ASC' or upper-case($Start/pfx:SearchReq/pfx:sort_order)='DESC'))">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="&quot;SortOrder can only accept 'ASC' and 'DESC' as value&quot;"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/sortOrderErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:when test="$Start/pfx:SearchReq/pfx:page_number&lt;=0">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="'page number should be greater then 0'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/pageLessErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:when test="not(cust:matchRegex($Start/pfx:SearchReq/pfx:page_number, '[0-9]'))">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="'page number should only contain numbers'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/pageValidErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:when test="$Start/pfx:SearchReq/pfx:page_size&lt;=0">
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="'Page size sould be greater then 0'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/limitLessErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:when>
                <xsl:otherwise>
                    <pfx2:ActivityInput>
                        <message>
                            <xsl:value-of select="'page size should be less then or equal to 100'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns2:GlobalVariables/ErrorCodes/limitGreaterErrCode,$_globalVariables/ns2:GlobalVariables/CodeSep,$_globalVariables/ns2:GlobalVariables/badReqCode)"/>
                        </messageCode>
                    </pfx2:ActivityInput>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>551</pd:x>
        <pd:y>241</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="ErrLogHandler">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>688</pd:x>
        <pd:y>235</pd:y>
        <config>
            <processName>/LoggerProcesses/ErrLogHandler.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns:ErrorLogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'SearchRetrival'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'SearchCatalog'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'ERROR'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'failed'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="$SetCorrID/root/param"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'ErrorDetails'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd', current-date())"/>
                </ns:Timestamp>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <ns:ErrorCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </ns:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <ns:ErrorMsg>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns:ErrorMsg>
                </xsl:if>
            </ns:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>SetCorrID</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SetCorrID</pd:from>
        <pd:to>SearchCatalogReqLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SearchCatalogReqLog</pd:from>
        <pd:to>SearchCatalogSub</pd:to>
        <pd:xpathDescription>validation</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>string-length($Start/pfx:SearchReq/pfx:Search_Query)!=0
and
string-length($Start/pfx:SearchReq/pfx:Search_Type)!=0
and
(upper-case($Start/pfx:SearchReq/pfx:sort_by)='TITLE' or upper-case($Start/pfx:SearchReq/pfx:sort_by)='AUTHOR')
and
(upper-case($Start/pfx:SearchReq/pfx:sort_order)='ASC' or upper-case($Start/pfx:SearchReq/pfx:sort_order)='DESC')
and
$Start/pfx:SearchReq/pfx:page_number&gt;0
and
cust:matchRegex($Start/pfx:SearchReq/pfx:page_number, '[0-9]')
and
$Start/pfx:SearchReq/pfx:page_size&gt;0
and 
$Start/pfx:SearchReq/pfx:page_size&lt;=100</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>SearchCatalogSub</pd:from>
        <pd:to>SearchCatalogResLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SearchCatalogResLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SearchCatalogReqLog</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>ErrLogHandler</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ErrLogHandler</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>