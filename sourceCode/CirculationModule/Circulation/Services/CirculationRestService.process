<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>Services/CirculationRestService.process</pd:name>
    <pd:startName>HTTP Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="HTTP Receiver">
        <pd:type>com.tibco.plugin.http.HTTPEventSource</pd:type>
        <pd:resourceType>httppalette.httpEventSource</pd:resourceType>
        <pd:x>152</pd:x>
        <pd:y>105</pd:y>
        <config>
            <outputMode>String</outputMode>
            <defaultEncoding>ISO8859_1</defaultEncoding>
            <inputOutputVersion>5.3.0</inputOutputVersion>
            <newMimeSupport>true</newMimeSupport>
            <sharedChannel>/SharedResources/Connections/Http/HTTP Connection.sharedhttp</sharedChannel>
            <Headers>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Accept" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Accept-Charset" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Accept-Encoding" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Length" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Connection" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Cookie" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </Headers>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>533</pd:endX>
    <pd:endY>111</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1679051907675</pd:targetNamespace>
    <pd:activity name="REST Dispatch and Reply">
        <pd:type>com.tibco.plugin.json.rest.server.activities.RestAdapterActivity</pd:type>
        <pd:resourceType>rest.activities.RestAdapter</pd:resourceType>
        <pd:x>334</pd:x>
        <pd:y>109</pd:y>
        <config>
            <eventSource>HTTP Receiver</eventSource>
            <RestService>
                <InnerService>
                    <ns0:application xmlns:ns0="http://wadl.dev.java.net/2009/02" name="Application">
                        <ns0:resources name="Circulation" base="%%Rest/baseURL%%">
                            <ns0:resource id="Register" path="/register">
                                <ns0:doc/>
                                <ns0:method id="register" name="POST">
                                    <ns0:doc/>
                                    <Binding process="/Processess/RestProcesses/RegisterPatronRest.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Input-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" match="/*">
                                                <root>
                                                    <payload>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/PostData"/>
                                                    </payload>
                                                </root>
                                            </xsl:template>
                                        </Input-Binding>
                                        <Output-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.org/Circulation" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.org/Circulation" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                                <RESTResponse>
                                                    <xsl:if test="$End/root/payload">
                                                        <asciiContent>
                                                            <xsl:value-of select="$End/root/payload"/>
                                                        </asciiContent>
                                                    </xsl:if>
                                                    <Headers>
                                                        <StatusLine>
                                                            <xsl:value-of select="if (exists($RegisterPatronMain/group/ns:RegisterPatronsRes)) then 'HTTP/1.1 200 OK'&#xA; else&#xA; if (contains($RegisterPatronMain/group/ns:fault/ns:FaultCode, '-400')) then 'HTTP/1.1 400 Bad Request' else 'HTTP/1.1 500 Internal Server Error'"/>
                                                        </StatusLine>
                                                        <Content-Type>
                                                            <xsl:value-of select="'application/json'"/>
                                                        </Content-Type>
                                                    </Headers>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                    <ns0:request name="RegistePatronReq">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:request>
                                    <ns0:response name="RegistePatronRes" status="200">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:response>
                                </ns0:method>
                            </ns0:resource>
                            <ns0:resource id="BookCheckout" path="/checkout">
                                <ns0:doc/>
                                <ns0:method id="checkout" name="POST">
                                    <ns0:doc/>
                                    <Binding process="/Processess/RestProcesses/CheckOutRest.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Input-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <root>
                                                    <payload>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/PostData"/>
                                                    </payload>
                                                </root>
                                            </xsl:template>
                                        </Input-Binding>
                                        <Output-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.org/Circulation" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.org/Circulation" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                                <RESTResponse>
                                                    <xsl:if test="$End/root/payload">
                                                        <asciiContent>
                                                            <xsl:value-of select="$End/root/payload"/>
                                                        </asciiContent>
                                                    </xsl:if>
                                                    <Headers>
                                                        <StatusLine>
                                                            <xsl:value-of select="if (exists($CheckOutMain/group/ns:BookCheckOutRes)) then 'HTTP/1.1 200 OK'&#xA; else&#xA; if (contains($CheckOutMain/group/ns:fault/ns:FaultCode, '-400')) then 'HTTP/1.1 400 Bad Request' else 'HTTP/1.1 500 Internal Server Error'"/>
                                                        </StatusLine>
                                                        <Content-Type>
                                                            <xsl:value-of select="'application/json'"/>
                                                        </Content-Type>
                                                    </Headers>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                    <ns0:request name="CheckOutReq">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:request>
                                    <ns0:response name="CheckOutRes" status="200">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:response>
                                </ns0:method>
                            </ns0:resource>
                            <ns0:resource id="CheckIN" path="/checkin">
                                <ns0:method id="checkIn" name="POST">
                                    <ns0:doc/>
                                    <Binding process="/Processess/RestProcesses/CheckInRest.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Input-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <root>
                                                    <payload>
                                                        <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/PostData"/>
                                                    </payload>
                                                </root>
                                            </xsl:template>
                                        </Input-Binding>
                                        <Output-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.org/Circulation" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.org/Circulation" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <RESTResponse>
                                                    <xsl:if test="$End/root/payload">
                                                        <asciiContent>
                                                            <xsl:value-of select="$End/root/payload"/>
                                                        </asciiContent>
                                                    </xsl:if>
                                                    <Headers>
                                                        <StatusLine>
                                                            <xsl:value-of select="if ($CheckInMain/group/ns:fault/ns:FaultCode=200) then 'HTTP/1.1 200 OK'&#xA; else&#xA; if (contains($CheckInMain/group/ns:fault/ns:FaultCode, '-400')) then 'HTTP/1.1 400 Bad Request' else 'HTTP/1.1 500 Internal Server Error'"/>
                                                        </StatusLine>
                                                        <Content-Type>
                                                            <xsl:value-of select="'application/json'"/>
                                                        </Content-Type>
                                                    </Headers>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                    <ns0:request name="CheckInReq">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:request>
                                    <ns0:response name="CheckInRes" status="200">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:response>
                                </ns0:method>
                                <ns0:doc/>
                            </ns0:resource>
                        </ns0:resources>
                    </ns0:application>
                </InnerService>
            </RestService>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInputClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/json">
                <tmp>
                    <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/Method"/>
                </tmp>
            </ns0:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>HTTP Receiver</pd:from>
        <pd:to>REST Dispatch and Reply</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>REST Dispatch and Reply</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>