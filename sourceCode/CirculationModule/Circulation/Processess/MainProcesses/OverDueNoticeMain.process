        <pd:activity name="genNoticeID">
            <pd:type>com.tibco.plugin.jdbc.JDBCGeneralActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCGeneralActivity</pd:resourceType>
            <pd:x>833</pd:x>
            <pd:y>245</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <maxRows>100</maxRows>
                <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            </config>
            <pd:inputBindings>
                <jdbcGeneralActivityInput>
                    <statement>
                        <xsl:value-of select="&quot;select 'N'||NoticeIDSEQ.NEXTVAL as NOTICEID from dual&quot;"/>
                    </statement>
                </jdbcGeneralActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="fetchPatronInfo">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>989</pd:x>
            <pd:y>245</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <maxRows>100</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
                <statement>SELECT CARDNUMBER,PATRON_NAME,EMAIL
  FROM LIB_CARD where CARDNUMBER=?
</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>CARDNUMBER</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <QueryOutputCachedSchemaColumns>CARDNUMBER</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>PATRON_NAME</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>EMAIL</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <oraObjects/>
                <oraTables/>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput>
                    <CARDNUMBER>
                        <xsl:choose>
                            <xsl:when test="exists($currentElement/pfx:OverdueInfo/pfx:CARDNUMBER)">
                                <xsl:value-of select="$currentElement/pfx:OverdueInfo/pfx:CARDNUMBER"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </CARDNUMBER>
                </jdbcQueryActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Map-Data-1">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>1152</pd:x>
            <pd:y>246</pd:y>
            <config>
                <element ref="pfx:Notice"/>
            </config>
            <pd:inputBindings>
                <pfx:Notice>
                    <xsl:if test="$genNoticeID/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[name='NOTICEID']/value">
                        <pfx:notice_id>
                            <xsl:value-of select="$genNoticeID/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[name='NOTICEID']/value"/>
                        </pfx:notice_id>
                    </xsl:if>
                    <xsl:if test="$currentElement/pfx:OverdueInfo/pfx:CARDNUMBER">
                        <pfx:library_card_number>
                            <xsl:value-of select="$currentElement/pfx:OverdueInfo/pfx:CARDNUMBER"/>
                        </pfx:library_card_number>
                    </xsl:if>
                    <xsl:if test="$fetchPatronInfo/resultSet/Record[1]/PATRON_NAME">
                        <pfx:Name>
                            <xsl:value-of select="$fetchPatronInfo/resultSet/Record[1]/PATRON_NAME"/>
                        </pfx:Name>
                    </xsl:if>
                    <xsl:if test="$currentElement/pfx:OverdueInfo/pfx:BOOKID">
                        <pfx:book_id>
                            <xsl:value-of select="$currentElement/pfx:OverdueInfo/pfx:BOOKID"/>
                        </pfx:book_id>
                    </xsl:if>
                    <pfx:notice_date>
                        <xsl:value-of select="tib:format-date('yyyy-MM-dd', current-date())"/>
                    </pfx:notice_date>
                    <xsl:if test="$fetchPatronInfo/resultSet/Record[1]/EMAIL">
                        <pfx:EmailID>
                            <xsl:value-of select="$fetchPatronInfo/resultSet/Record[1]/EMAIL"/>
                        </pfx:EmailID>
                    </xsl:if>
                    <pfx:status>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/status/Sent"/>
                    </pfx:status>
                </pfx:Notice>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Send Mail">
            <pd:type>com.tibco.plugin.mail.MailPubActivity</pd:type>
            <pd:resourceType>ae.activities.MailActivityResource</pd:resourceType>
            <pd:x>1291</pd:x>
            <pd:y>247</pd:y>
            <config>
                <newMimeSupport>true</newMimeSupport>
                <inputOutputVersion>5.2.0</inputOutputVersion>
                <authenticate>true</authenticate>
                <host>%%Connections/SMTP/Host%%</host>
                <username>%%Connections/SMTP/username%%</username>
                <password>%%Connections/SMTP/password%%</password>
                <InputHeaders>
                    <xsd:element name="root">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </InputHeaders>
            </config>
            <pd:inputBindings>
                <ns0:mailActivityInput>
                    <from>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/fromEmail"/>
                    </from>
                    <xsl:if test="$Map-Data-1/pfx:Notice/pfx:EmailID">
                        <to>
                            <xsl:value-of select="$Map-Data-1/pfx:Notice/pfx:EmailID"/>
                        </to>
                    </xsl:if>
                    <subject>
                        <xsl:value-of select="'Library Borrowing Notice'"/>
                    </subject>
                    <ns12:bodyElement>
                        <bodyText>
                            <xsl:value-of select="concat('&lt;html>&#xD;&#xA;&lt;head/>&#xD;&#xA;&lt;body>&#xD;&#xA;&lt;p>Hi ',$Map-Data-1/pfx:Notice/pfx:Name,',','&#xD;&#xA;&#x9;&#x9;&#x9;this mail is to let you know that the due date for the book you borrowed is over. we request you to return the book immediatly&#xD;&#xA;&lt;table border=&quot;1&quot;>&#xD;&#xA;  &lt;tr>&#xD;&#xA;    &lt;td>NOTICE ID&lt;/td>&#xD;&#xA;    &lt;td>',$Map-Data-1/pfx:Notice/pfx:notice_id,'&lt;/td>&#xD;&#xA;  &lt;/tr>&#xD;&#xA;  &lt;tr>&#xD;&#xA;    &lt;td>Library CARD NUMBER&lt;/td>&#xD;&#xA;    &lt;td>',$Map-Data-1/pfx:Notice/pfx:library_card_number,'&lt;/td>&#xD;&#xA;  &lt;/tr>&#xD;&#xA;  &lt;tr>&#xD;&#xA;    &lt;td>NAME&lt;/td>&#xD;&#xA;   &#xA; &lt;td>',$Map-Data-1/pfx:Notice/pfx:Name,'&lt;/td>&#xD;&#xA;  &lt;/tr>&#xD;&#xA;  &lt;tr>&#xD;&#xA;    &lt;td>BOOK_ID&lt;/td>&#xD;&#xA;    &lt;td>',$Map-Data-1/pfx:Notice/pfx:book_id,'&lt;/td>&#xD;&#xA;  &lt;/tr>&#xD;&#xA;  &lt;tr>&#xD;&#xA;    &lt;td>NOTICE_DATE&lt;/td>&#xD;&#xA;    &lt;td>',$Map-Data-1/pfx:Notice/pfx:notice_date,'&lt;/td>&#xD;&#xA;  &lt;/tr>&#xD;&#xA;&lt;/table>&#xD;&#xA;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#xA;Thanks and Regards,&#xD;&#xA;&#x9;&#xA;Library Management.&#xD;&#xA;&lt;/p>&lt;/body>&#xD;&#xA;&lt;/html>')"/>
                        </bodyText>
                    </ns12:bodyElement>
                    <Headers>
                        <Content-Type>
                            <xsl:value-of select="&quot;text/html&quot;"/>
                        </Content-Type>
                    </Headers>
                </ns0:mailActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="fetchBorrwingDetails">
