    <pd:activity name="insertAllNotice">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>1488</pd:x>
        <pd:y>246</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>INSERT INTO LIB_NOTICEINFO
  VALUES (?,?,?,?,?,?,?)</statement>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>notice_id</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>library_card_number</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Name</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>book_id</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>notice_date</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>EmailID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>status</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <batchUpdate>true</batchUpdate>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <xsl:for-each select="$Accumulate/AccumulatedOutput/pfx:Notice">
                    <Record>
                        <notice_id>
                            <xsl:value-of select="pfx:notice_id"/>
                        </notice_id>
                        <library_card_number>
                            <xsl:value-of select="pfx:library_card_number"/>
                        </library_card_number>
                        <Name>
                            <xsl:value-of select="pfx:Name"/>
                        </Name>
                        <book_id>
                            <xsl:value-of select="pfx:book_id"/>
                        </book_id>
                        <notice_date>
                            <xsl:value-of select="pfx:notice_date"/>
                        </notice_date>
                        <EmailID>
                            <xsl:value-of select="pfx:EmailID"/>
                        </EmailID>
                        <status>
                            <xsl:value-of select="pfx:status"/>
                        </status>
                    </Record>
                </xsl:for-each>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>632</pd:x>
        <pd:y>213</pd:y>
        <config>
            <element ref="pfx:overDueRoot"/>
        </config>
        <pd:inputBindings>
            <pfx:overDueRoot>
                <xsl:for-each select="$fetchBorrwingDetails/resultSet/Record[tib:compare-date(tib:parse-date('yyyy-MM-dd', DUE_DATE), current-date()) = '-1']">
                    <pfx:OverdueInfo>
                        <pfx:TRANID>
                            <xsl:value-of select="TRANID"/>
                        </pfx:TRANID>
                        <xsl:if test="BOOKID">
                            <pfx:BOOKID>
                                <xsl:value-of select="BOOKID"/>
                            </pfx:BOOKID>
                        </xsl:if>
                        <xsl:if test="CARDNUMBER">
                            <pfx:CARDNUMBER>
                                <xsl:value-of select="CARDNUMBER"/>
                            </pfx:CARDNUMBER>
                        </xsl:if>
                        <xsl:if test="LOAN_DATE">
                            <pfx:LOAN_DATE>
                                <xsl:value-of select="LOAN_DATE"/>
                            </pfx:LOAN_DATE>
                        </xsl:if>
                        <xsl:if test="DUE_DATE">
                            <pfx:DUE_DATE>
                                <xsl:value-of select="DUE_DATE"/>
                            </pfx:DUE_DATE>
                        </xsl:if>
                        <xsl:if test="RETURN_DATE">
                            <pfx:RETURN_DATE>
                                <xsl:value-of select="RETURN_DATE"/>
                            </pfx:RETURN_DATE>
                        </xsl:if>
                        <xsl:if test="STATUS">
                            <pfx:STATUS>
                                <xsl:value-of select="STATUS"/>
                            </pfx:STATUS>
                        </xsl:if>
                    </pfx:OverdueInfo>
                </xsl:for-each>
            </pfx:overDueRoot>
        </pd:inputBindings>
    </pd:activity>
