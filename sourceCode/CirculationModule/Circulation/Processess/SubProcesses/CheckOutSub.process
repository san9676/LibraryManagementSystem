<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.example.org/Circulation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.example.org/Circulation" schemaLocation="/SharedResources/Schemas/Circulation.xsd"/>
    <pd:name>Processess/SubProcesses/CheckOutSub.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:BookCheckOutReq"/>
    <pd:startX>133</pd:startX>
    <pd:startY>100</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$Map-Data/pfx:BookCheckOutRes"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:BookCheckOutRes"/>
    <pd:endX>909</pd:endX>
    <pd:endY>103</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1679053339790</pd:targetNamespace>
    <pd:activity name="FetchLibInfo">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>227</pd:x>
        <pd:y>103</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>select CARDNUMBER,PATRON_NAME from LIB_CARD where CARDNUMBER=?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>CARDNUMBER</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <QueryOutputCachedSchemaColumns>CARDNUMBER</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>PATRON_NAME</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <CARDNUMBER>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:BookCheckOutReq/pfx:library_card_number)">
                            <xsl:value-of select="$Start/pfx:BookCheckOutReq/pfx:library_card_number"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </CARDNUMBER>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SQL Direct">
        <pd:type>com.tibco.plugin.jdbc.JDBCGeneralActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCGeneralActivity</pd:resourceType>
        <pd:x>477</pd:x>
        <pd:y>104</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcGeneralActivityInput>
                <statement>
                    <xsl:value-of select="&quot;select 'T'||TranNoSeq.NEXTVAL as TRANID from dual&quot;"/>
                </statement>
            </jdbcGeneralActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>568</pd:x>
        <pd:y>103</pd:y>
        <config>
            <element ref="pfx:BookCheckOutRes"/>
        </config>
        <pd:inputBindings>
            <pfx:BookCheckOutRes>
                <xsl:if test="$SQL-Direct/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[name='TRANID']/value">
                    <pfx:transaction_id>
                        <xsl:value-of select="$SQL-Direct/jdbcGeneralActivityOutput/unknownResultset[1]/row[1]/column[name='TRANID']/value"/>
                    </pfx:transaction_id>
                </xsl:if>
                <xsl:if test="$Start/pfx:BookCheckOutReq/pfx:book_id">
                    <pfx:book_id>
                        <xsl:value-of select="$Start/pfx:BookCheckOutReq/pfx:book_id"/>
                    </pfx:book_id>
                </xsl:if>
                <pfx:due_date>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd',tib:add-to-date(current-date(),0,0, 14))"/>
                </pfx:due_date>
                <pfx:status>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/status/checkOut"/>
                </pfx:status>
            </pfx:BookCheckOutRes>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="insertBorrowing">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>667</pd:x>
        <pd:y>107</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>INSERT INTO LIB_BORROWING
  VALUES (?,?,?,?,?,?,?)</statement>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>TRANID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>BOOKID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>CARDNUMBER</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>LOAN_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>DUE_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>RETURN_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>STATUS</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <TRANID>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:transaction_id)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:transaction_id"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </TRANID>
                <BOOKID>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:book_id)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:book_id"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </BOOKID>
                <CARDNUMBER>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:BookCheckOutReq/pfx:library_card_number)">
                            <xsl:value-of select="$Start/pfx:BookCheckOutReq/pfx:library_card_number"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </CARDNUMBER>
                <LOAN_DATE>
                    <xsl:value-of select="tib:format-date('yyyy-MM-dd',current-date())"/>
                </LOAN_DATE>
                <DUE_DATE>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:due_date)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:due_date"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </DUE_DATE>
                <RETURN_DATE xsi:nil="true"/>
                <STATUS>
                    <xsl:choose>
                        <xsl:when test="exists($Map-Data/pfx:BookCheckOutRes/pfx:status)">
                            <xsl:value-of select="$Map-Data/pfx:BookCheckOutRes/pfx:status"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </STATUS>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>279</pd:x>
        <pd:y>201</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <xsl:when test="count($FetchLibInfo/resultSet/Record)=0">
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="'Library card Number is not valid... please Try agian.'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns1:GlobalVariables/ErrorCodes/CheckOUT/LibNoRecErr, $_globalVariables/ns1:GlobalVariables/CodeSep,$_globalVariables/ns1:GlobalVariables/BadReq)"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:when>
                <xsl:otherwise>
                    <ns:ActivityInput>
                        <message>
                            <xsl:value-of select="'This Card number has already borrowed 3 books...., Please Return the book to borrow new Book'"/>
                        </message>
                        <messageCode>
                            <xsl:value-of select="concat($_globalVariables/ns1:GlobalVariables/ErrorCodes/CheckOUT/CountMore3, $_globalVariables/ns1:GlobalVariables/CodeSep,$_globalVariables/ns1:GlobalVariables/BadReq)"/>
                        </messageCode>
                    </ns:ActivityInput>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="JDBC Call Procedure">
        <pd:type>com.tibco.plugin.jdbc.JDBCCallActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCCallActivity</pd:resourceType>
        <pd:x>776</pd:x>
        <pd:y>110</pd:y>
        <config>
            <timeout>10</timeout>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <ProcedureName>UPDATEAVAILBOOK</ProcedureName>
            <schema/>
            <oraObjects/>
            <oraTables/>
            <parameterTypes>
                <parameter>
                    <colName>BOOK_ID</colName>
                    <colType>1</colType>
                    <dataType>12</dataType>
                    <typeName>VARCHAR2</typeName>
                </parameter>
                <parameter>
                    <colName>RESPONSE</colName>
                    <colType>4</colType>
                    <dataType>12</dataType>
                    <typeName>VARCHAR2</typeName>
                </parameter>
            </parameterTypes>
        </config>
        <pd:inputBindings>
            <inputs>
                <inputSet>
                    <BOOK_ID>
                        <xsl:choose>
                            <xsl:when test="exists($Start/pfx:BookCheckOutReq/pfx:book_id)">
                                <xsl:value-of select="$Start/pfx:BookCheckOutReq/pfx:book_id"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </BOOK_ID>
                </inputSet>
            </inputs>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="FetchBorrowing">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>359</pd:x>
        <pd:y>104</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>select TRANID from LIB_BORROWING where CARDNUMBER=? and STATUS=?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>CardNum</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Status</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <QueryOutputCachedSchemaColumns>TRANID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <CardNum>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx:BookCheckOutReq/pfx:library_card_number)">
                            <xsl:value-of select="$Start/pfx:BookCheckOutReq/pfx:library_card_number"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </CardNum>
                <Status>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/status/checkOut"/>
                </Status>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>FetchLibInfo</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FetchLibInfo</pd:from>
        <pd:to>FetchBorrowing</pd:to>
        <pd:xpathDescription>recordAvailable</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($FetchLibInfo/resultSet/Record)&gt;0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>SQL Direct</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>insertBorrowing</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FetchLibInfo</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertBorrowing</pd:from>
        <pd:to>JDBC Call Procedure</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Call Procedure</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FetchBorrowing</pd:from>
        <pd:to>SQL Direct</pd:to>
        <pd:xpathDescription>count&gt;3</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($FetchBorrowing/resultSet/Record) &lt; 3</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>FetchBorrowing</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>